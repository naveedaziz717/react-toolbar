{
  "compilerOptions": {
    "importHelpers": true,
    "noImplicitAny": true,
    "removeComments": false,
    "declaration": true,
    "outDir": "../../dist/es2015",
    "lib": ["dom", "es6"],
    "target": "es5",
    "module": "es2015",
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "moduleResolution": "node",
    "types": ["react", "jest"]
  },
  "include": ["../../src/**/*.ts", "../../src/**/*.tsx"]
}
