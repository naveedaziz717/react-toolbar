{
  "name": "ts-react-toolbox",
  "version": "0.2.7",
  "description": "Opinionated toolbox boilerplate to create Typescript React components",
  "main": "src/index.js",
  "repository": "git@github.com:zzarcon/ts-react-toolbox.git",
  "author": "Hector Leon Zarco Garcia <hezarco@gmail.com>",
  "license": "MIT",
  "bin": {
    "ts-react-toolbox": "./bin/ts-react-toolbox"
  },
  "scripts": {
    "build": "tsc",
    "add_permission": "chmod +x ./bin/ts-react-toolbox.js",
    "cli": "./bin/ts-react-toolbox.js",
    "ts:cli": "yarn tsc && yarn cli",
    "rename_bin": "mv bin/ts-react-toolbox.js bin/ts-react-toolbox",
    "release": "npm version patch && git push --tags && git push && npm publish",
    "clean": "rm -rf src/**/*.js",
    "prepublishOnly": "yarn clean && yarn build && yarn rename_bin"
  },
  "dependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.15",
    "@types/node": "10",
    "@types/react": "16.8.7",
    "@types/react-dom": "^16.9.1",
    "@types/styled-components": "^4.1.19",
    "@types/webpack": "^4.1.2",
    "@types/webpack-dev-server": "^2.9.4",
    "awesome-typescript-loader": "^5.0.0-1",
    "chalk": "^2.3.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.4.0",
    "gh-pages": "^1.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.11.1",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^16.10.0",
    "react-dom": "^16.10.0",
    "react-gh-corner": "^1.1.2",
    "styled-components": "^4.4.0",
    "tmp": "^0.0.33",
    "tslib": "^1.9.3",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.5.1",
    "typescript": "^3.5.2",
    "webpack": "^4.36.1",
    "webpack-bundle-analyzer": "^2.11.2",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.1"
  },
  "files": [
    "src",
    "bin",
    "static",
    "static/.travis.yml",
    "jest.config.js",
    "preprocessor.js",
    "setupTests.js",
    "tsconfig.json",
    "tsconfig.es5.json",
    "tsconfig.es2015.json",
    "configs"
  ],
  "keywords": [
    "toolbox",
    "react",
    "ts",
    "typescript",
    "config",
    "webpack",
    "webpack-server",
    "lint",
    "tslint",
    "format",
    "test",
    "deploy",
    "release"
  ],
  "devDependencies": {
    "@types/tmp": "^0.0.33"
  }
}
