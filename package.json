    "gh-pages": "^3.1.0",
    "jest": "^27.4.7",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-gh-corner": "^1.1.3",
    "tmp": "^0.0.33",
    "ts-jest": "^27.1.2",
