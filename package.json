{
  "name": "ts-react-toolbox",
  "version": "1.0.0",
  "description": "Opinionated toolbox boilerplate to create Typescript React components",
  "main": "src/index.js",
  "repository": "git@github.com:zzarcon/ts-react-toolbox.git",
  "author": "Hector Leon Zarco Garcia <hezarco@gmail.com>",
  "license": "MIT",
  "bin": {
    "ts-react-toolbox": "./bin/ts-react-toolbox"
  },
  "scripts": {
    "build": "tsc",
    "add_permission": "chmod +x ./bin/ts-react-toolbox.js",
    "cli": "./bin/ts-react-toolbox.js",
    "ts:cli": "yarn tsc && yarn cli",
    "rename_bin": "mv bin/ts-react-toolbox.js bin/ts-react-toolbox",
    "release": "npm version patch && git push --tags && git push && npm publish",
    "clean": "rm -rf src/**/*.js",
    "prepublishOnly": "yarn clean && yarn build && yarn rename_bin",
    "postinstall": "rm -rf node_modules/@types/react-native",
    "test": "echo \"no tests\""
  },
  "dependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/gh-pages": "^3.0.0",
    "@types/jest": "^26.0.9",
    "@types/node": "^12.7.8",
    "@types/react": "16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.0",
    "@types/webpack": "^4.41.21",
    "@types/webpack-dev-server": "3.11.0",
    "awesome-typescript-loader": "^5.0.0-1",
    "chalk": "^2.3.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "gh-pages": "^3.1.0",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-gh-corner": "^1.1.2",
    "styled-components": "^5.1.1",
    "tmp": "^0.0.33",
    "ts-jest": "^26.1.4",
    "tslib": "^2.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "files": [
    "src",
    "bin",
    "static",
    "static/.travis.yml",
    "jest.config.js",
    "setupTests.js",
    "tsconfig.json",
    "tsconfig.es5.json",
    "tsconfig.es2015.json",
    "configs"
  ],
  "keywords": [
    "toolbox",
    "react",
    "ts",
    "typescript",
    "config",
    "webpack",
    "webpack-server",
    "lint",
    "tslint",
    "format",
    "test",
    "deploy",
    "release"
  ],
  "devDependencies": {
    "@types/tmp": "^0.0.33"
  },
  "resolutions": {
    "@types/node": "12.7.8"
  }
}
